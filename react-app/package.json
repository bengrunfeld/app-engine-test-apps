{
  "name": "vanillajs-app",
  "version": "1.0.0",
  "description": "A Vanilla JS app for Google App Engine",
  "main": "./src/index.js",
  "scripts": {
    "deploy": "gcloud app deploy",
    "buildServer": "webpack --config webpack.server.config.js",
    "dev": "rm -rf dist && npm run buildServer && webpack --config webpack.dev.config.js",
    "build": "rm -rf dist && npm run buildServer && webpack --mode production",
    "devServer": "node ./dist/server.js",
    "start": "node ./dist/server.js"
  },
  "engines": {
    "node": ">=4.3.2"
  },
  "author": "Ben Grunfeld",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "requiresKeyFile": true,
  "requiresProjectId": true,
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-node-externals": "^1.7.2"
  }
}
